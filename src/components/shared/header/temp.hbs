<div class="navbar navbar-fixed-top" role="navigation">
  {{!log this level="error"}}
  <div class="container">
    <ul class="navbar-right-ul accountUser">
      <li class="navbar-right">
        <div class="menuOptions" id="menuOptions">
          <span class="accountOptions hidden-xs hidden-sm">my acorn tv</span>
          <span id="clicker">
            <img
              class="accountIcon"
              width="18"
              src="//api.rlje.net/acorn/artwork/size/account-icon?t=Icons"
            />
          </span>
        </div>

        <ul class="drop-select closed" style="display:none;">
          <a href="//{{environment}}acorn.tv/browse/recentlywatched/">
            <li>RECENTLY WATCHED</li>
          </a>
          <a href="//{{environment}}acorn.tv/browse/yourwatchlist/">
            <li>MY WATCHLIST</li>
          </a>
          <a href="#accountStatus">
            <li>MANAGEACCOUNT</li>
          </a>
          <a href="#editPassword">
            <li>CHANGE PASSWORD</li>
          </a>
          {{#if Customer.webPaymentEdit}}
            {{#if Customer.StripeEnabled}}
              <a href="updatecard.html?stripe_id={{Customer.StripeCustomerID}}">
                <li>UPDATE BILLING INFORMATION</li>
              </a>
            {{/if}}
            <a href="#editEmail">
              <li>CHANGE EMAIL</li>
            </a>
          {{/if}}
          {{#if Session.LoggedIn}}
            <a href="#logout">
              <li>LOG OUT</li>
            </a>
          {{else}}
            <a href="{{signinEnv}}/signin.jsp?OperationalScenario=STORE">
              <li>SIGN IN</li>
            </a>
          {{/if}}
        </ul>
      </li>
    </ul>

    <div class="navbar-header">
      <a href="//{{environment}}acorn.tv/"><img
          src="/img/atvlogo.png"
          class="atv-logo"
        /></a>
      <button
        data-toggle="collapse-side"
        data-target=".side-collapse"
        data-target-2=".side-collapse-container"
        type="button"
        class="navbar-toggle"
      >
        <div>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </div>
      </button>
    </div>

    <div class="navbar-collapse side-collapse in">
      <ul class="nav navbar-nav">
        <li><a href="//{{environment}}acorn.tv/">HOME</a></li>
        <li><a href="//{{environment}}acorn.tv/browse">BROWSE</a></li>
        <li><a href="//{{environment}}acorn.tv/schedule">SCHEDULE</a></li>
        <li><a href="//support.acorn.tv">HELP</a></li>
        <li><a
            id="store-link"
            href="//{{storeEnv}}"
            style="color:#fff;display:block;"
          >STORE</a>
        </li>
      </ul>
    </div>
  </div>
</div>
<div id="jqueryScript"></div>
